package com.demo;
import java.util.*;
public class Demo {
	public static void main(String[] args) {
		List<String> names=new ArrayList<>();
		
		names.add("foo");
		names.add("bar");
		names.add("jar");
		names.add("car");
		
		System.out.println("-------------before sorting---------");
		for(String name: names) {
			System.out.println(name);
		}
		Collections.sort(names);
		
		System.out.println("-------------after sorting---------");
		for(String name: names) {
			System.out.println(name);
		}
		
		
	}

}
